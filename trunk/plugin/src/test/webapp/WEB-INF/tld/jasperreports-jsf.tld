            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
        </attribute>
        <attribute>
            <description><![CDATA[HTML frame width.]]></description>
            <name>width</name>
            <required>false</required>
            <deferred-value>
                <type>java.lang.String</type>
            </deferred-value>
        </attribute>
        <attribute>
            <description><![CDATA[
Any value representing the source for the report
file, source or compiled to be processed.
            ]]></description>
            <name>value</name>
            <required>true</required>
            <deferred-value>
                <type>java.lang.Object</type>
            </deferred-value>
        </attribute>
        <attribute>
            <description><![CDATA[
Specific validator instance used to override the
default behaviour of the plugin.
            ]]></description>
            <name>validator</name>
            <required>false</required>
            <deferred-value>
                <type>net.sf.jasperreports.jsf.validation.Validator</type>
            </deferred-value>
        </attribute>
    </tag>

