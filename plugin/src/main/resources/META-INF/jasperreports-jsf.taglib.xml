<!DOCTYPE facelet-taglib PUBLIC
"-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
"http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>
    <namespace>
        http://jasperreportjsf.sf.net/tld/jasperreports-jsf-1_0.tld
    </namespace>

    <tag>
        <tag-name>dataBroker</tag-name>
        <component>
            <component-type>
                net.sf.jasperreports.jsf.DataBroker
            </component-type>
        </component>
    </tag>

    <tag>
        <tag-name>reportFrame</tag-name>
        <component>
            <component-type>
                net.sf.jasperreports.jsf.HtmlReportFrame
            </component-type>
            <renderer-type>
                net.sf.jasperreports.jsf.Frame
            </renderer-type>
        </component>
    </tag>

    <tag>
        <tag-name>reportLink</tag-name>
        <component>
            <component-type>
                net.sf.jasperreports.jsf.HtmlReportLink
            </component-type>
            <renderer-type>
                net.sf.jasperreports.jsf.Link
            </renderer-type>
        </component>
    </tag>

</facelet-taglib>