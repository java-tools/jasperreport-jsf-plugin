<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
    
    <lifecycle>
    	<phase-listener>
    		net.sf.jasperreports.jsf.ReportPhaseListener
    	</phase-listener>
    </lifecycle>
    
    <managed-bean>
    	<managed-bean-name>ReportLinkTest</managed-bean-name>
    	<managed-bean-class>
    		net.sf.jasperreports.jsf.test.bean.ReportTestBean
    	</managed-bean-class>
    	<managed-bean-scope>request</managed-bean-scope>
    	
    	<managed-property>
    		<property-name>facturaId</property-name>
    		<value>1</value>
    	</managed-property>
    </managed-bean>
    
</faces-config>